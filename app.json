{
  "expo": {
    "name": "Juergen",
    "description": "",
    "slug": "Juergen",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "splash": {
      "image": "./assets/camera.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "nyc.lumberdev.juergen",
      "icon": "./assets/camera-icon-ios.png",
      "supportsTablet": true
    },
    "android": {
      "package": "nyc.lumberdev.juergen",
      "icon": "./assets/camera-icon-android.png",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}